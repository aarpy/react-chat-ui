{
  "name": "react-hot-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS project with hot code reloading",
  "scripts": {
    "start": "node server.js",
    "lint": "eslint src",
    "test": "npm run mocha",
    "test:watch": "npm run mocha:watch",
    "mocha": "mocha --compilers js:babel-core/register src/__tests__ --recursive --require ignore-styles",
    "mocha:watch": "npm run mocha -- --watch",
    "karma": "NODE_ENV=production karma start karma.config.js",
    "karma:watch": "npm run karma -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-hot-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Dan Abramov <dan.abramov@me.com> (http://github.com/gaearon)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gaearon/react-hot-boilerplate/issues"
  },
  "homepage": "https://github.com/gaearon/react-hot-boilerplate",
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-jest": "^15.0.0",
    "babel-loader": "^6.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.1",
    "css-loader": "^0.24.0",
    "enzyme": "^2.4.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.6.2",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "font-awesome": "^4.6.3",
    "ignore-styles": "^5.0.1",
    "isparta": "^4.0.0",
    "isparta-loader": "^2.0.0",
    "jest-cli": "^15.1.1",
    "jsdom": "^9.5.0",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "less-loader": "^2.2.3",
    "minimist": "^1.2.0",
    "mocha": "^3.0.2",
    "node-sass": "~3.8.0",
    "phantomjs-prebuilt": "^2.1.12",
    "pre-commit": "^1.1.3",
    "react-addons-test-utils": "^15.3.1",
    "react-hot-loader": "^1.3.0",
    "sass-bulk-import-loader": "^0.1.1",
    "sass-loader": "~4.0.0",
    "sinon": "git+https://github.com/sinonjs/sinon.git",
    "style-loader": "^0.13.1",
    "webpack": "~1.13.2",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "compass-mixins": "~0.12.10",
    "concat-stream": "^1.5.2",
    "file-loader": "^0.9.0",
    "flux": "^2.1.1",
    "font-awesome": "^4.6.3",
    "google-map-react": "^0.19.0",
    "react": "~15.3.1",
    "react-addons-css-transition-group": "~15.3.1",
    "react-addons-update": "^15.3.1",
    "react-controllables": "^0.6.0",
    "react-dom": "~15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
    "redux-ui": "0.0.15",
    "url-loader": "^0.5.7"
  }
}
